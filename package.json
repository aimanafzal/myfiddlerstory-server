{
  "name": "pwa-react-boilerplate",
  "version": "0.1.5",
  "engines": {
    "node": "6.0"
  },
  "dependencies": {
    "aws-sdk": "^2.477.0",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.5",
    "express-session": "^1.15.6",
    "firebase": "^5.8.0",
    "firebase-admin": "^7.0.0",
    "google-auto-auth": "^0.10.1",
    "google-drive": "0.0.8",
    "googleapis": "^40.0.0",
    "history": "^3.0.0",
    "jsonwebtoken": "^8.4.0",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "nodemailer": "^5.1.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-token-google": "^0.1.2",
    "shortid": "^2.2.14",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "node server.js"
  },
  "devDependencies": {
    "ava": "^1.2.1",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-react-app": "^2.0.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint-config-xo": "^0.17.0",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-plugin-react": "^6.9.0",
    "file-loader": "0.9.0",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.26.0",
    "minimist": "^1.2.0",
    "react-addons-test-utils": "^15.4.2",
    "style-loader": "0.13.1",
    "sw-precache": "^4.3.0",
    "sw-precache-webpack-dev-plugin": "^0.1.3",
    "sw-precache-webpack-plugin": "^0.7.2",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^3.2.1",
    "xo": "^0.17.1"
  },
  "xo": {
    "envs": [
      "browser"
    ],
    "extends": [
      "xo",
      "xo-react"
    ],
    "rules": {
      "quote-props": [
        "error",
        "as-needed"
      ],
      "react/require-optimization": 0,
      "react/forbid-component-props": 0,
      "import/no-unresolved": 0,
      "import/no-unassigned-import": 0,
      "unicorn/filename-case": 0
    }
  },
  "ava": {
    "files": "test/*.js",
    "require": "babel-register",
    "babel": "inherit"
  }
}
